/* 模板小图样式 */

.groupPhoto-page .templet-mini-photo-container {
  width: 100%;
  height: 240rpx;
  background: rgba(248, 248, 248, 1);
  box-shadow: 0px -1px 0px 0px rgba(220, 220, 220, 1);
  position: fixed;
  bottom: 0;
  left: 0;
}

.groupPhoto-page .iphoneXStyle {
  bottom: 140rpx;
}

.groupPhoto-page .templet-mini-photo-container .contain {
  width: 100%;
  display: flex;
}

.groupPhoto-page .templet-mini-photo-container .contain .item {
  margin: 20rpx 7rpx 20rpx 8rpx;
  box-sizing: border-box;
  border: 4rpx solid transparent;
}

.groupPhoto-page .templet-mini-photo-container .contain .active {
  border: 4rpx solid #fa6964;
}

.groupPhoto-page .templet-mini-photo-container .contain .item image {
  width: 160rpx;
  height: 200rpx;
}

