/* 模板6的样式 */

.groupPhoto-page .photo-templet-bg-6 .user-canvas6 {
  width: 330rpx;
  height: 330rpx;
  overflow: hidden;
  /* position: absolute; *//* top: 189rpx; *//* left: 14rpx; *//* background: red; */
}

.moveView {
  width: 330rpx;
  height: 330rpx;
  position: absolute;
  left: 0;
  top: 0;
  margin: 20rpx 0 0 20rpx;
  z-index: 9999;
}

.groupPhoto-page .photo-templet-bg-6 .bg {
  position: relative;
}

.groupPhoto-page .photo-templet-bg-6 .bg .bg-image {
  z-index: 100;
  width: 720rpx;
  height: 720rpx;
  position: absolute;
  left: -20rpx;
  top: 0;
}

.groupPhoto-page .photo-templet-bg-6 .bg .container {
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  z-index: 99;
  display: flex;
  justify-content: space-between;
  width: 720rpx;
  height: 100%;
}

.groupPhoto-page .photo-templet-bg-6 .bg .container .user-canvas6 {
  height: 330rpx;
  margin: 20rpx 0 0 20rpx;
}

.groupPhoto-page .photo-templet-bg-6 .bg .container .star-canvas1 {
  height: 330rpx;
  margin: 0 20rpx 20rpx 0;
}

.groupPhoto-page .photo-templet-bg-6 .bg .container .item:nth-child(2) {
  align-self: flex-end;
}

