/* 合成按钮样式 */

.groupPhoto-page .synthesis-btn {
  width: 600rpx;
  height: 100rpx;
  background: rgba(56, 191, 123, 1);
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 75rpx;
  bottom: 20vh;
}
.groupPhoto-page .synthesis-btn-iphoneXStyle{
  bottom: 30vh;
}
.groupPhoto-page .synthesis-btn text {
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(249, 249, 249, 1);
}

